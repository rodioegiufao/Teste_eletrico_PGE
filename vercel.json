{
  "version": 2,
  "builds": [
    {
      "src": "ifc-viewer.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/ifc-viewer.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.ifc)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/octet-stream"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
